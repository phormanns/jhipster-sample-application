entity Post {
	title String required,
	shortText String,
	fullText String
}

entity Tag {
	name String required
}

entity Blog {
	title String required,
	description String
}

relationship ManyToMany {
	Tag{post(title)} to Post{tag(name)}
}

relationship OneToMany {
	Blog{post(title)} to Post{blog(title)}
}

paginate Tag, Post with infinite-scroll
paginate Blog with pagination

service all with serviceImpl
